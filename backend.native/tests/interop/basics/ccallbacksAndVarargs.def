

---

struct S {
    int x;
};

static int getX(struct S (*callback)(void)) {
    return callback().x;
}

static void applyCallback(struct S s, void (*callback)(struct S)) {
    callback(s);
}

static struct S makeS(int x, ...) {
    return (struct S){ x };
}

enum E {
    ZERO, ONE, TWO
};

static enum E makeE(int ordinal, ...) {
    return ordinal;
}